# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET         /exp                   ch.isbsib.proteomics.mzviz.controllers.ExperimentalController.stats
GET         /exp/msrun/list        ch.isbsib.proteomics.mzviz.controllers.ExperimentalController.listMSRunIds
POST        /exp/msrun             ch.isbsib.proteomics.mzviz.controllers.ExperimentalController.loadMSRun
DELETE      /exp/msrun/:id         ch.isbsib.proteomics.mzviz.controllers.ExperimentalController.deleteMSRun(id)